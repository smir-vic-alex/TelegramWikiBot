{
  "$id": "http://example.com/example.json",
  "type": "object",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "additionalProperties": false,
  "properties": {
    "token": {
      "description": "telegram bot token",
      "type": "string",
      "minLength": 40
    },
    "name": {
      "description": "telegram bot name",
      "type": "string",
      "minLength": 1
    }
  },
  "required": ["token", "name"]
}